#:kivy 2.3.0

<FUNRegister>:
    FloatLayout:

#--- Label box in top left corner ---------------------------------------------------------------------------------------#
        BoxLayout:
            orientation: 'vertical'
            size_hint: None, None
            size: '200dp', '100dp'
            pos_hint: {'x': 0, 'top': 1}
            padding: 10
            MyLabel:
                text: 'Register cars:'
                bold: True
                halign: 'left'

#------------ Bottom Navigate buttons -----------------------------------------------------------------------------------#

        # Size and position
        BoxLayout:
            size_hint: None, None
            size: '200dp', '40dp'
            pos_hint: {'x': 0, 'y': 0}

            # Back to funstart
            MyButton:
                text: 'Back'
                background_color: 0.8, 0.1, 0.1, 1
                color: 1, 1, 1, 1
                on_press: root.go_to_funstart()

            # Next to funwarmup
            MyButton:
                text: 'Next'
                background_color: 0.1, 0.8, 0.1, 1
                color: 1, 1, 1, 1
                on_press: root.go_to_funwarmup()

#-----------------------------------------------------------------------------------------------------------------------#
